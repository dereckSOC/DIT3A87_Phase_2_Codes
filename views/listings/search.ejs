                                <label for="semidetached">Semi-Detached</label><br>
                                <input type="checkbox" id="detached" name="propType" value="detached" <% if(propTypeArr.includes('detached')) {%> checked <%} %>>
                                <label for="detached">Detached</label><br>
                            </div>  
                        </div>

        
