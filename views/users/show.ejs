<%- include("../partials/header") %>

  <!-- banner -->
  <div class="container px-0"
    style="background: url('<%= user.banner %>'); height: 300px; width: 100%; background-size: cover; object-fit: cover; background-position: center center; border-bottom-left-radius: 10px; border-bottom-right-radius: 10px;">
    <div class="justify-content-between align-items-center d-flex">
      <div class="px-4 mx-1">
      </div>
      <div>
        <!-- approve/remove agent -->
        <% if(user.isAgent && currentUser && currentUser.isAdmin){ %>
          <form action="/dashboard/agents/<%= user._id %>?_method=PUT" method="POST" class="text-center"
            style="position:relative; top: 10px;">
            <% if(user.agentStatus==false){ %>
              <button type="submit" name="user[agentStatus]" value="true" class="btn approveBtn"
                style="background: #41e792; color: white; font-weight: 600; font-size: 0.9em;">Approve Agent</button>
              <% } else{ %>
                <button type="submit" name="user[agentStatus]" value="false" class="btn approveBtn"
                  style="background: #DC3545; color: white; font-weight: 600; font-size: 0.9em;">Remove Agent</button>
                <% } %>
          </form>
          <% } %>
      </div>
      <div>
        <!-- upload banner button -->
        <% if(currentUser && user._id.equals(currentUser._id) || currentUser && currentUser.isAdmin){ %>
          <form action="/user/<%=user._id %>/banner?_method=PUT" method="POST" enctype="multipart/form-data"
            id="bannerForm">
            <span class="justify-content-end d-flex">
              <label for="file-upload" class="customBannerUpload px-3">
                <i class="fas fa-camera"></i>
              </label>
              <input type="file" id="file-upload" name="banner" onchange="this.form.submit()" id="bannerUpload"
                accept="image/*">
            </span>
          </form>
          <% } else { %>
            <div class="py-4"></div>
            <% } %>
      </div>
    </div>

    <!-- profile image -->
    <span class="justify-content-center d-flex">
      <img src="<%= user.image %>" alt="user profile image" class="profileImage">
    </span>
    <% if(currentUser && user._id.equals(currentUser._id) || currentUser && currentUser.isAdmin){ %>
      <!-- upload profile image button -->
      <form action="/user/<%=user._id %>/image?_method=PUT" method="POST" enctype="multipart/form-data" id="imageForm"
        class="text-center">
        <label for="file-upload-image" class="customImageUpload px-3">
          <i class="fas fa-camera"></i>
        </label>
        <input type="file" id="file-upload-image" name="image" onchange="this.form.submit()" id="imageUpload"
          accept="image/*">
      </form>
      <% } %>
  </div>



  <!-- follow & chat & edit profile -->
  <div class="container align-items-center justify-content-between d-flex mt-3 px-4">
    <div>
      <% if(!currentUser){ %>
        <a href="/follow/<%= user.id %>">
          <button class="btn btn-outline-dark followBtn">Follow</button>
        </a>
        <% } else { %>
          <% if(user._id.equals(currentUser._id)){ %>
            <a class="btn btn-outline-dark profileOverviewBtn" href="/user/<%= user._id %>/edit">
              <span>Edit Profile</span>
            </a>
            <% } else { %>
              <div class="left">
                <a href="/follow/<%= user.id %>">
                  <button class="btn btn-outline-dark followBtn">Follow</button>
                </a>
              </div>
              <div class="left right">
                <form action="http://localhost:8800/api/conversations/<%= currentUser._id %>/<%= user.id %>"
                  method="POST">
                  <button type="submit" class="btn btn-outline-dark chatBtn">Chat</button>
                </form>
              </div>
              <% } %>
                <% } %>
    </div>
    <div class="justify-content-center d-flex py-2">
      <div class="left">
        <span id="userRole">
          <i class="fas fa-user pr-1"></i>
          <% if(user.isAgent==true){ %>
            Agent
            <% } else if(user.isAdmin==true){ %>
              Admin
              <% } else { %>
                Seeker
                <% } %>
        </span>
      </div>
      <div class="left right">
        <span>
          <% if(user.agentStatus==true || user.isAdmin==true){ %>
            <span id="userStatus"><i class="fas fa-check pr-1"></i>Verified</span>
            <% } else { %>
              <% if(user.isAgent==true){ %>
                <span id="userStatus"><i class="fas fa-clock pr-1"></i>Pending</span>
                <% } else { %>
                  <% } %>
                    <% } %>
        </span>
      </div>
      <% if(user.isAgent==true){ %>
        <div class="right">
          <span id="ceaNum">
            <span class="pr-1" style="letter-spacing: -0.15em;">CEA</span>
            <%= user.cea %>
          </span>
          <br>
        </div>
        <% } %>
    </div>
  </div>

  <!-- user details -->
  <div class="container">
    <p id="profileUsername" class="text-center pt-1 my-0">
      <%= user.username %>
    </p>
    <% if (user.rating===0) { %>
      <span class="justify-content-center d-flex pb-2 my-0" style="font-size: 0.9em; color: gray;">
        <em>No reviews yet.</em>
      </span>
      <% } else { %>
        <div class="text-center mb-3">
          <p class="m-0">
            <%= user.rating.toFixed(2) %>
              <span class="fa fa-star checked"></span>
              <span class="fa fa-star <% if (user.rating > 1.5) { %> checked <% } %>"></span>
              <span class="fa fa-star <% if (user.rating > 2.5) { %> checked <% } %>"></span>
              <span class="fa fa-star <% if (user.rating > 3.5) { %> checked <% } %>"></span>
              <span class="fa fa-star <% if (user.rating > 4.5) { %> checked <% } %>"></span>
              (<%= user.reviews.length %>)
          </p>
        </div>
        <% } %>
  </div>

  <!-- overview -->
  <div class="container overview my-4 text-center">
    <a href="/dashboard/listings">
      <span style="border-right: solid lightgray 1px;">
        <%= listings.length %>
          <% if(listings.length<2){ %>
            Listing
            <% } else { %>
              Listings
              <% } %>
      </span>
    </a>

    <a href="/user/<%= user._id %>/reviews" data-toggle="modal" data-target="#reviewTarget">
      <span style="border-right: solid lightgray 1px;">
        <%= user.reviews.length %>
          <% if(user.reviews.length<2){ %>
            Review
            <% } else { %>
              Reviews
              <% } %>
      </span>
    </a>

    <a href="" data-toggle="modal" data-target="#notificationTarget">
      <span style="border-right: solid lightgray 1px;">
        <%= allNotifications.length %>
          <% if(allNotifications.length<2){ %>
            Notification
            <% } else { %>
              Notifications
              <% } %>
      </span>
    </a>

    <a href="" data-toggle="modal" data-target="#followerTarget">
      <span>
        <%= allFollowers.length %>
          <% if(allFollowers.length<2){ %>
            Follower
            <% } else { %>
              Followers
              <% } %>
      </span>
    </a>
  </div>



  <div class="container">
    <div class="row">
      <!-- profile details & followers & notifictions -->
      <div class="col-4">
        <div class="row">
          <!-- profile details -->
          <div class="col-12 borderStyle">
            <div class="align-items-center d-flex pl-4 pr-2 py-2 mb-4 justify-content-between headerStyle">
              <div class="left">
                <p class="profileHeader my-0">Profile Details</p>
              </div>
              <div class="right">
                <% if(currentUser && user._id.equals(currentUser._id) || currentUser && currentUser.isAdmin){ %>
                  <a class="btn btn-outline-dark profileBtn" href="/user/<%= user._id %>/edit">
                    <span>Edit</span>
                  </a>
                  <% } %>
              </div>
            </div>
            <div class="row px-4" id="profileDetails">
              <div class="col-3">
                <span>Joined:</span>
              </div>
              <div class="col-9">
                <p>
                  <%= moment(user.createdAt).format('DD MMMM YYYY, h:mm:ss a'); %>
                </p>
              </div>
              <div class="col-3">
                <span>Role:</span>
              </div>
              <div class="col-9">
                <p>
                  <% if(user.isAgent==true){ %>
                    Agent
                    <% } else if(user.isAdmin==true){ %>
                      Admin
                      <% } else { %>
                        Seeker
                        <% } %>
                </p>
              </div>
              <div class="col-3">
                <span>Name:</span>
              </div>
              <div class="col-9">
                <p>
                  <%= user.firstName + " " + user.lastName %>
                </p>
              </div>
              <div class="col-3">
                <span>Email:</span>
              </div>
              <div class="col-9">
                <p>
                  <a href="mailto:<%= user.email %>" style="color: black;">
                    <%= user.email %>
                  </a>
                </p>
              </div>
              <% if(user.isAgent==true){ %>
                <div class="col-3">
                  <span>CEA:</span>
                </div>
                <div class="col-9">
                  <p>
                    <%= user.cea %>
                  </p>
                </div>
                <% } %>
                  <div class="col-3">
                    <span>Gender:</span>
                  </div>
                  <div class="col-9">
                    <p>
                      <%= user.gender %>
                    </p>
                  </div>
                  <div class="col-3">
                    <span>Phone:</span>
                  </div>
                  <div class="col-9">
                    <p>
                      <%= user.phone %>
                    </p>
                  </div>
                  <% if(currentUser && user._id.equals(currentUser._id)|| currentUser && currentUser.isAdmin){ %>
                    <div class="col-3">
                      <span>Address:</span>
                    </div>
                    <div class="col-9">
                      <p>
                        <%= user.streetName %><br>
                          <%= user.unitNumber %><br>
                            <%= user.country %><br>
                              <%= user.state %><br>
                                <%= user.postalCode %>
                      </p>
                    </div>
                    <% } %>
            </div>
          </div>
          <!-- followers -->
          <div class="col-12 borderStyle">
            <div class="align-items-center d-flex pl-4 pr-2 py-2 justify-content-between headerStyle">
              <div class="left">
                <p class="profileHeader my-0">Followers</p>
              </div>
              <div class="right">
                <button type="button" class="btn btn-outline-dark profileBtn" data-toggle="modal"
                  data-target="#followerTarget">
                  View All
                </button>
              </div>
              <!-- Modal -->
              <div class="modal fade" id="followerTarget" tabindex="-1" role="dialog"
                aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered" role="document">
                  <div class="modal-content">
                    <div class="justify-content-between align-items-center d-flex border-bottom">
                      <div class="py-4"></div>
                      <div>
                        <p class="modal-title pl-5" id="exampleModalLongTitle">Followers</p>
                      </div>
                      <div>
                        <button type="button" class="close mr-2" data-dismiss="modal" aria-label="Close">
                          <span aria-hidden="true">&times;</span>
                        </button>
                      </div>
                    </div>
                    <div class="modal-body">
                      <% if(user.followers=="" ){ %>
                        <p>
                          <em>No followers yet.</em>
                        </p>
                        <% } else { %>
                          <% allFollowers.reverse().forEach(function(followers, index) { %>
                            <div>
                              <div class="row align-items-center d-flex mb-3">
                                <div class="col-2">
                                  <a href="/user/<%= followers._id %>">
                                    <img src="<%= followers.image %>"
                                      style="width:60px; border: 1px solid gray; border-radius: 50%; object-fit: cover;">
                                  </a>
                                </div>
                                <div class="col-7">
                                  <a href="/user/<%= followers._id %>" style="color: black">
                                    <%= followers.username %>
                                  </a>
                                </div>
                                <!-- follow button -->
                                <div class="col-3 justify-content-end d-flex">
                                  <a href="/follow/<%= followers._id %>">
                                    <button class="btn btn-outline-dark editDeleteReviewBtn">Follow</button>
                                  </a>
                                </div>
                              </div>
                            </div>
                            <% }); %>
                              <% } %>
                    </div>
                  </div>
                </div>
              </div>

            </div>
            <div class="px-2">
              <% if(user.followers=="" ){ %>
                <p class="mt-3 px-3">
                  <em>No followers yet.</em>
                </p>
                <% } else { %>
                  <% allFollowers.reverse().slice(0, 3).forEach(function(followers, index) { %>
                    <div class="pt-3">
                      <div class="row align-items-center d-flex">
                        <div class="col-3">
                          <a href="/user/<%= followers._id %>">
                            <img src="<%= followers.image %>"
                              style="width:60px; border: 1px solid gray; border-radius: 50%; object-fit: cover;">
                          </a>
                        </div>
                        <div class="col-6 pr-0">
                          <a href="/user/<%= followers._id %>" style="color: black">
                            <%= followers.username %>
                          </a>
                        </div>
                        <div class="col-3 px-0">
                          <a href="/follow/<%= followers._id %>">
                            <button class="btn btn-outline-dark editDeleteReviewBtn">Follow</button>
                          </a>
                        </div>
                      </div>
                    </div>
                    <% }); %>
                      <% } %>
            </div>
          </div>
          <!-- notifications -->
          <% if(currentUser){ %>
            <div class="col-12 borderStyle">
              <div class="align-items-center d-flex pl-3 pr-2 py-2 justify-content-between headerStyle">
                <div class="left pl-2">
                  <p class="profileHeader my-0">Notifications</p>
                </div>
                <!-- Button trigger modal -->
                <div class="right">
                  <button type="button" class="btn btn-outline-dark notificationBtn" data-toggle="modal"
                    data-target="#notificationTarget">
                    View Past
                  </button>
                </div>

                <!-- Modal -->
                <div class="modal fade" id="notificationTarget" tabindex="-1" role="dialog"
                  aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                  <div class="modal-dialog modal-dialog-centered" role="document">
                    <div class="modal-content">
                      <div class="justify-content-between align-items-center d-flex border-bottom">
                        <div class="py-4"></div>
                        <div>
                          <p class="modal-title pl-5" id="exampleModalLongTitle">Notifications</p>
                        </div>
                        <div>
                          <button type="button" class="close mr-2" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                          </button>
                        </div>
                      </div>
                      <div class="modal-body">
                        <% if(allNotifications==0 ){ %>
                          <p>
                            <em>No notifications yet.</em>
                          </p>
                          <% } else { %>
                            <% allNotifications.reverse().forEach(function(notification) { %>
                              <div class="row mb-3 align-items-center d-flex">
                                <div class="col-2">
                                  <a href="/user/<%= notification.idUser %>">
                                    <img src="<%= notification.image %>" alt="listing image"
                                      style="width:60px; border: 1px solid gray; border-radius: 50%; object-fit: cover;">
                                  </a>
                                </div>
                                <div class="col-7 pr-0">
                                  <a href="/notifications/<%= notification.id %>" style="color:black;">
                                    <%= notification.username %> added a new listing
                                  </a><br>
                                  <span style="color: gray; font-size: 0.9em;">
                                    <%= moment(notification.createdAt).fromNow() %>
                                  </span>
                                </div>
                                <div class="col-3 justify-content-end d-flex">
                                  <a href="/notifications/<%= notification.id %>">
                                    <!-- <button class="btn btn-outline-dark followersViewBtn">View</button> -->
                                    <img src="<%= notification.listingImage %>"
                                      style="width:60px; border: 1px solid gray; border-radius: 10px; object-fit: cover;">
                                  </a>
                                </div>
                              </div>
                              <% }); %>
                                <% } %>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="px-2">
                <% if(notifications==0 ){ %>
                  <p class="mt-3 px-3">
                    <em>No new notifications yet.</em>
                  </p>
                  <% } else { %>
                    <% allNotifications.reverse().slice(0, 3).forEach(function(notification) { %>
                      <% if(notification.isRead==false){ %>
                        <div class="row mt-3 align-items-center d-flex">
                          <div class="col-3">
                            <a href="/user/<%= notification.idUser %>">
                              <img src="<%= notification.image %>"
                                style="width:60px; border: 1px solid gray; border-radius: 50%; object-fit: cover;">
                            </a>
                          </div>
                          <div class="col-9">
                            <a href="/notifications/<%= notification.id %>" style="color:black;">
                              <%= notification.username %> added a new listing
                            </a><br>
                            <span style="color: gray; font-size: 0.9em;">
                              <%= moment(notification.createdAt).fromNow() %>
                            </span>
                          </div>
                          <!-- <div class="col-3">
                            <a href="/notifications/<%= notification.id %>">
                              <button class="btn btn-outline-dark followersViewBtn">View</button>
                            </a>
                          </div> -->
                        </div>
                        <% } %>
                          <% }); %>
                            <% } %>
              </div>
            </div>
            <% } %>
        </div>
      </div>

      <!-- listings & reviews -->
      <div class="col-8">
        <!-- listings -->
        <div class="col-12 borderStyle">
          <div class="align-items-center d-flex pl-4 pr-2 py-2 mb-4 justify-content-between headerStyle">
            <div class="left">
              <p class="profileHeader my-0">Listings</p>
            </div>
            <div class="right">
              <a class="btn btn-outline-dark profileBtn" href="/dashboard/listings">
                <span>View All</span>
              </a>
            </div>
          </div>
          <% if(user.isAgent==true || user.isAdmin==true){ %>
            <% if(listings==0){ %>
              <p class="px-4">
                <em>No listings yet.</em>
              </p>
              <% } else { %>
                <% listings.reverse().slice(0, 3).forEach(function(listing) { %>
                  <div class="row align-items-center d-flex mt-2 px-2">
                    <div class="col-2">
                      <a href="/listings/<%= listing.id %>">
                        <img src="<%= listing.thumbnail %>"
                          style="background-size: cover; width: 85px; height: 85px; object-fit: cover; background-position: center center; border-radius: 25px; border: 1px solid gray;">
                      </a>
                    </div>
                    <div class="col-6 pl-5">
                      <a href="/listings/<%= listing.id %>" style="color: black; font-weight: 600;">
                        <%= listing.name %><br>
                      </a>
                      <span style="color: gray; font-size: 0.9em;">
                        <%= moment(listing.createdAt).fromNow() %>
                      </span>
                    </div>
                    <div class="col-4 justify-content-end d-flex">
                      <%if(currentUser&&listing.author.id.equals(currentUser._id)||currentUser&&currentUser.isAdmin){%>
                        <div class="left">
                          <a class="btn btn-outline-dark profileListingEdit"
                            href="/listings/<%= listing._id %>/edit">Edit</a>
                        </div>
                        <div class="right">
                          <form action="/listings/<%= listing._id %>?_method=DELETE" method="POST">
                            <button class="btn btn-outline-dark profileListingDelete">Delete</button>
                          </form>
                        </div>
                        <% } %>
                    </div>
                  </div>
                  <% }); %>
                    <% } %>
                      <% } %>
        </div>
        <!-- reviews -->
        <div class="col-12 borderStyle">
          <div class="align-items-center d-flex pl-4 pr-2 py-2 justify-content-between headerStyle">
            <div class="left">
              <p class="profileHeader my-0">Reviews</p>
            </div>
            <div>
              <% if(currentUser && currentUser.isAgent){ %>
                <% } else { %>
                  <div class="left">
                    <div>
                      <a class="btn btn-outline-dark reviewBtn <% if (currentUser && user.reviews.some(function (review) {return review.author.id.equals(currentUser._id)})) { %> disabled <% } %>"
                        href="/user/<%= user._id %>/reviews/new">
                        Write
                      </a>
                    </div>
                  </div>
                  <% } %>
                    <div class="right">
                      <button type="button" class="btn btn-outline-dark reviewBtn" data-toggle="modal"
                        data-target="#reviewTarget">
                        View All
                      </button>
                    </div>

                    <!-- Modal -->
                    <div class="modal fade" id="reviewTarget" tabindex="-1" role="dialog"
                      aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                      <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                        <div class="modal-content">
                          <div class="justify-content-between align-items-center d-flex border-bottom">
                            <div class="py-4"></div>
                            <div>
                              <p class="modal-title pl-5" id="exampleModalLongTitle">Reviews</p>
                            </div>
                            <div>
                              <button type="button" class="close mr-2" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                              </button>
                            </div>
                          </div>
                          <div class="modal-body">
                            <% if (user.rating===0) { %>
                              <div class="px-3">
                                <em>No reviews yet.</em>
                              </div>
                              <% } else { %>
                                <% user.reviews.reverse().forEach(function(review){ %>
                                  <div class="row">
                                    <div class="col-12">
                                      <div class="row">
                                        <div class="col-2 text-center">
                                          <a href="/user/<%=user._id %>">
                                            <img src="<%= review.author.image %>"
                                              style="width: 85px; height: 85px; border-radius: 50%; border: 1px solid gray;">
                                          </a>
                                        </div>
                                        <div class="col-10">
                                          <p style="text-align: justify; white-space: pre-wrap;" class="mb-0 border-bottom pb-2"><%=review.text%></p>
                                          <div class="row align-items-center d-flex pt-2">
                                            <div class="col-8">
                                              <p class="mb-0">
                                                Review by:<a href="/user/<%=user._id %>">
                                                  <strong>
                                                    <%= review.author.username %>
                                                  </strong>
                                                </a>
                                              </p>
                                              <p class="mb-0">
                                                <%- '<span class="fa fa-star checked"></span>' .repeat(review.rating) %>
                                                  <%- '<span class="fa fa-star"></span>' .repeat(5 - review.rating) %>
                                              </p>
                                              <p class="m-0">
                                                <em>
                                                  <%= moment(review.createdAt).fromNow() %>
                                                </em>
                                              </p>
                                            </div>
                                            <div class="col-4 justify-content-end d-flex">
                                              <%if(currentUser&&review.author.id.equals(currentUser._id)||currentUser&&currentUser.isAdmin){%>
                                                <div class="left">
                                                  <a class="btn btn-outline-dark editDeleteReviewBtn"
                                                    href="/user/<%=user._id %>/reviews/<%=review._id %>/edit">Edit</a>
                                                </div>
                                                <div class="right">
                                                  <form id="delete-form"
                                                    action="/user/<%=user._id %>/reviews/<%=review._id %>?_method=DELETE"
                                                    method="POST">
                                                    <input type="submit"
                                                      class="btn btn-outline-dark editDeleteReviewBtn" value="Delete">
                                                  </form>
                                                </div>
                                                <% } %>
                                            </div>
                                          </div>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                                  <% }); %>
                                    <% } %>
                          </div>
                        </div>
                      </div>
                    </div>
            </div>
          </div>
          <div class="px-2">
            <% if (user.rating===0) { %>
              <div class="mt-3 px-3">
                <em>No reviews yet.</em>
              </div>
              <% } else { %>
                <% user.reviews.reverse().slice(0, 3).forEach(function(review){ %>
                  <div class="row mt-3">
                    <div class="col-12">
                      <div class="row">
                        <div class="col-2 text-center">
                          <a href="/user/<%=user._id %>">
                            <img src="<%= review.author.image %>"
                              style="width: 85px; height: 85px; border-radius: 50%; border: 1px solid gray;">
                          </a>
                        </div>
                        <div class="col-10">
                          <p style="text-align: justify; white-space: pre-wrap;" class="mb-0 pb-2 border-bottom"><%=review.text%></p>
                          <div class="row align-items-center d-flex pt-2">
                            <div class="col-8">
                              <p class="mb-0">
                                Review by:<a href="/user/<%=user._id %>">
                                  <strong>
                                    <%= review.author.username %>
                                  </strong>
                                </a>
                              </p>
                              <p class="mb-0">
                                <%- '<span class="fa fa-star checked"></span>' .repeat(review.rating) %>
                                  <%- '<span class="fa fa-star"></span>' .repeat(5 - review.rating) %>
                              </p>
                              <p class="m-0">
                                <em>
                                  <%= moment(review.createdAt).fromNow() %>
                                </em>
                              </p>
                            </div>
                            <div class="col-4 justify-content-end d-flex">
                              <%if(currentUser&&review.author.id.equals(currentUser._id)||currentUser&&currentUser.isAdmin){%>
                                <div class="left">
                                  <a class="btn btn-outline-dark editDeleteReviewBtn"
                                    href="/user/<%=user._id %>/reviews/<%=review._id %>/edit">Edit</a>
                                </div>
                                <div class="right">
                                  <form id="delete-form"
                                    action="/user/<%=user._id %>/reviews/<%=review._id %>?_method=DELETE" method="POST">
                                    <input type="submit" class="btn btn-outline-dark editDeleteReviewBtn"
                                      value="Delete">
                                  </form>
                                </div>
                                <% } %>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <% }); %>
                    <% } %>
          </div>
        </div>
      </div>
    </div>
  </div>

  <%- include("../partials/footer") %>